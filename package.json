{
  "name": "bjorn.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun velite --watch & next dev",
    "build": "bun download-themes && next telemetry disable && bun velite build && next build",
    "start": "next start",
    "lint": "eslint 'src/{components,app,utils,providers,constants,styles}/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/{components,app,utils,providers,constants,styles}/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --check \"src/{components,providers,constants,app,styles,utils,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:fix": "prettier --write \"src/{components,providers,constants,app,styles,utils,styles}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "types:check": "tsc --pretty --noEmit",
    "clean": "rimraf .next ** rimraf out && rimraf .velite",
    "download-themes": "node ./scripts/download-themes.mjs",
    "update-deps": "npx npm-check-updates -u && bun i",
    "prune-code": "ts-prune -i '.next|.velite|src/app'",
    "postinstall": "is-ci || husky install",
    "analyze": "ANALYZE=true bun run build"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "autoprefixer": "10.4.19",
    "axios": "^1.7.1",
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "colord": "^2.9.3",
    "drizzle-orm": "^0.30.10",
    "next": "14.2.3",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-compare-slider": "^3.1.0",
    "react-dom": "18.3.1",
    "react-twc": "^1.4.1",
    "react-tweet": "^3.2.1",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "3.4.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@next/bundle-analyzer": "^14.2.3",
    "@next/eslint-plugin-next": "^14.2.3",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/xml": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "million": "^3.0.6",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "remark-smartypants": "^3.0.1",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remark-unwrap-images": "^4.0.0",
    "sass": "^1.77.2",
    "tailwindcss-hocus": "^0.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "velite": "^0.1.0-rc.3",
    "xml": "^1.0.1"
  }
}
